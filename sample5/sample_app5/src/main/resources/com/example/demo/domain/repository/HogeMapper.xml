<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.example.demo.domain.repository.HogeMapper">

    <select id="getHoges" resultType="com.example.demo.domain.model.entity.HogeEntity">
        SELECT
			id,
			name
        FROM
            hoge
    </select>
    
</mapper>