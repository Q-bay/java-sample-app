<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.example.demo.domain.repository.HogeMapper">

    <select id="selectHoge" resultType="map">
        SELECT
			id,
			name
        FROM
            hoge
    </select>
    
    <select id="selectBuildings" resultType="map">
        SELECT
			building_id,
			building_name,
			building_detail
        FROM
            building
    </select>
    
    <select id="selectRooms" resultType="map">
        SELECT
			room_id,
			room_name
        FROM
            room
    </select>
</mapper>