<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.example.demo.domain.repository.RoomMapper">

    <select id="selectRooms" resultType="com.example.demo.domain.model.entity.RoomEntity">
        SELECT
			id,
			room_name,
			room_detail
        FROM
            room
    </select>
    
</mapper>